<% if session[:mobile] == "Y" %>
  <%= render 'mmonearticle' %>
<% else %>
  <div class='row'> 
    <div class='col-sm-offset-0 col-sm-12'>
      <div class="well">
        <%= render 'onearticle' %>
        <div class='gutter-0 col-sm-12 article-title'>
          <br>All Ads from <%= @article.user.name %><hr><br>
        </div>
        <%# params[:donotdisplay] = "Y" %>
        <%= render 'article', obj: @articles %>
        <div align="left">
          <%= will_paginate %>
        </div>
      </div>
    </div>
  </div>
  <% end %>
